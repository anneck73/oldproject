{# GLOBAL VARIABLES #}
{% set currentRoute = app.request.get('_route') %}
{# ################################################################################################################## #}
{% spaceless %}<!DOCTYPE html><!--// Copyright Mealmatch GmbH © 2016-2018. All rights reserved! {{ app_version }} //-->{% endspaceless %}
<html {{ sonata_seo_html_attributes() }} lang="{{ app.request.locale }}">
<head {{ sonata_seo_head_attributes() }}>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:copyright" name="copyright" content="Copyright Mealmatch GmbH © 2016-2018. All rights reserved!">
    <meta property="og:author" name="author" content="wizard@mealmatch.de">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    {{ sonata_seo_title() }}
    {{ sonata_seo_metadatas() }}
    {{ sonata_seo_link_canonical() }}
    {{ sonata_seo_lang_alternates() }}
    {{ sonata_seo_head_attributes() }}
    {% block base_twitter_image %}
    <!--// Twitter CARD //-->
    {% endblock %}
    <meta name="version" content="{{ app_version }}">
    {% block fbOpenGraphHead %}
    <!-- FB-OG -->
    <meta property="og:url" content="{{app.request.uri}}" />
    <meta property="og:image" content="{{  app.request.getSchemeAndHttpHost() ~ asset('bundles/mmwebfront/images/mm/mealmatch-logo.png') }}" />
    <!-- Favicon MS IE / When a site is pinned IE will look for this file. -->
    <meta name="msapplication-config" content="/browserconfig.xml" />
    {% endblock %}
    {# -------------------------------------------------------------------------------------------------------------- #}

    {#<!--// {{ encore_entry_link_tags('mmUI') }} //-->#}

    <!-- Bootstrap -->
    {% stylesheets
        output='css/mealmatch3rdparty.css' filter='?cssrewrite,scssphp'
        'bundles/mmwebfront/css/bootstrap.min.css'
        'bundles/mmwebfront/css/bootstrap-theme.min.css'
        'bundles/mmwebfront/css/bootstrap-datetimepicker.css'
        'bundles/mmwebfront/css/bootstrap-tagsinput-typeahead.css'
        'bundles/mmwebfront/css/bootstrap-tagsinput.css'
        'bundles/mmwebfront/css/bootstrap-dialog.css'
        'bundles/mmwebfront/css/datepicker.css'
        'bundles/fullcalendar/css/fullcalendar/fullcalendar.min.css'
        'bundles/mmwebfront/css/material-icons.css'
        'bundles/bmatznerfontawesome/css/font-awesome.css'
        'bundles/mmwebfront/scss/simple-line-icons.scss'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {# -------------------------------------------------------------------------------------------------------------- #}
    <!-- Mealmatch Globals -->
    {% stylesheets
        filter='?cssrewrite,scssphp' output='css/mealmatch.css'
        'bundles/mmwebfront/css/fonts.css'
        'bundles/mmwebfront/css/mealmatch-circle-icons.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {# -------------------------------------------------------------------------------------------------------------- #}
    <!-- WebApp Styles -->
    {% stylesheets
        filter='?cssrewrite,scssphp' output='css/webapp.css'
        'bundles/mmwebfront/sass/webapp.scss'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {# -------------------------------------------------------------------------------------------------------------- #}
    {# this stylesheet block should be overwritten by page styles inside blocks#}
    {% block stylesheets %}
        <!--// PAGE LEVEL CUSTOM HEAD CSS //-->
    {% endblock %}
    {# -------------------------------------------------------------------------------------------------------------- #}
    {# this javascript block can be orverrisdden by page specific JS. #}
    {% block jsHead %}
        <!--// PAGE LEVEL CUSTOM HEAD JavaScript //-->
    {% endblock %}
    <script> </script><!-- here to ensure a non-blocking load still occurs in IE and Edge, even if scripts follow loadCSS in head -->
    {# -------------------------------------------------------------------------------------------------------------- #}
    <!-- IE 9  -->
    <!--[if IE]><link rel="shortcut icon" href="/favicon.ico"><![endif]-->
    <link rel="icon" href="/favicon.png">
    <link rel="icon" type="image/x-icon" href="/{{ asset('favicon.ico') }}" />
    <!-- Apple  -->
    <link rel="apple-touch-icon-precomposed" href="/apple-icon-precomposed.png">
    <!-- iOS -->
    <link rel="apple-touch-icon" href="/apple-touch-icon.png">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--// GoogleAnalytics INIT JS //-->
    {% block GAHEAD %}
    {% spaceless %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
        </script>
    {% endspaceless %}
    {% endblock GAHEAD %}
    <!--// GoogleAnalytics INIT JS //-->
</head>
{# BASE HTML BODY #}
<body class="mm-body">
{% block flashbag %}{% include('MMWebFrontBundle:Partials:Flashbag.html.twig') %}{% endblock %}
<header class="fixed-header">
{% block navbar %}{{ include('@WEBUI/navbar.html.twig') }}{% endblock %}
</header>
<main class="content">
    <section class="main">
        {% block body %}{% endblock %}
    </section>
    <footer>
        {% block footer %}{{ include('@WEBUI/footer.html.twig') }}{% endblock %}
    </footer>
</main>
{# EOF BASE HTML BODY #}
<!--// 3rdParty JS //-->
{% javascripts
'bundles/fosjsrouting/js/router.js'
'bundles/mmwebfront/js/jquery-2.2.4.min.js'
'bundles/mmwebfront/js/bootstrap.min.js'
'bundles/mmwebfront/js/moment.min.js'
'bundles/mmwebfront/js/locale/de.js'
'bundles/mmwebfront/js/locale/en-gb.js'
'bundles/mmwebfront/js/bootstrap-tagsinput.min.js'
'bundles/mmwebfront/js/bootstrap-datetimepicker.min.js'
'bundles/mmwebfront/js/bootstrap-dialog.js'
'bundles/mmwebfront/js/bloodhound.js'
'bundles/mmwebfront/js/typeahead.js'
'bundles/mmwebfront/js/bootstrap3-typeahead.js'
'bundles/adesignscalendar/js/fullcalendar/fullcalendar.min.js'
'bundles/mmwebfront/js/fullcalendar/fullcalendar.settings.js'
'bundles/sideclickbootstrapmodal/js/sideclick-bootstrap-modal.js'
output='js/3rdParty.js'
filter='?jsqueeze'
%}
    {# Enables JS Routing in all TWIG templates #}
<script src="{{ asset_url }}"></script>
<script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
{% endjavascripts %}
{# Global JavaScripts #}
{% block javascripts %}
<!--// Mealmatch JS //-->
{% javascripts
'bundles/mmwebfront/js/mm-base.js'
output='js/mealmatch.js'
filter='?jsqueeze'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}
{# Block for PAGE specific JavaScript loaded after GLOBAL JavaScripts#}
<!--// Page JS //-->
{% block page_js %}
    {# Every PAGE is tracked with GA #}
{% endblock %}
    {% block GA %}{% spaceless %}<!--// Google Analytics //-->
    <script type="text/javascript">
        {% block GA_inner %}
        ga('create', '{{ mm.ga_tracking_id }}',
            'auto', { allowLinker: true } , 'variationsTracker-{{ app.session.get('variant') }}');
        ga('set', 'anonymizeIp', true);
        ga('send', 'pageview');
        ga('require', 'linker');
        ga('linker:autoLink', ['mealmatch.restaurant', 'mealmatch.de', 'www.mealmatch.de', 'www.mealmatch.restaurant']);

        {% endblock GA_inner %}</script><!--// Google Analytics //-->
{% endspaceless %}{% endblock GA %}
{# UI Specific and GLOBAL JavaScript, runs last because it works on HTML(UI) elements. #}
{% javascripts
'bundles/mmwebfront/js/mm-ui.js'
output='js/mealmatch-ui.js'
filter='?jsqueeze'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
<!--// Jira Collector //-->
{% spaceless %}
    <script type="text/javascript">
        jQuery.ajax({
            url: "https://mealmatch.atlassian.net/s/d41d8cd98f00b204e9800998ecf8427e-T/sit9rs/b/20/e73395c53c3b10fde2303f4bf74ffbf6/_/download/batch/com.atlassian.jira.collector.plugin.jira-issue-collector-plugin:issuecollector-embededjs/com.atlassian.jira.collector.plugin.jira-issue-collector-plugin:issuecollector-embededjs.js?locale=de-DE&collectorId=d403200a",
            type: "get",
            cache: true,
            dataType: "script"
        });
    </script>
{% endspaceless %}
<!--// JIRA Collector //-->
{% block modal %}
    {# Bootstrap-Modal Bundle#}
    <div class="modal fade" id="emptyModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
         aria-hidden="true" data-target="#emptyModal">
        <div class="modal-dialog">
        </div>
    </div>
{% endblock %}
</body>
